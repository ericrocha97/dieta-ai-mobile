{
  "expo": {
    "name": "dieta.ai",
    "slug": "dieta-ai",
    "scheme": "dieta-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F232C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F232C"
      }
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}